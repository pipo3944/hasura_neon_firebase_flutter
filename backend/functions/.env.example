# Firebase Cloud Functions Environment Variables
# Copy this to .env.yaml for local development

# Hasura GraphQL Endpoint
# Local: http://localhost:8080/v1/graphql
# Dev: https://hasura-dev-xxxxx.run.app/v1/graphql
# Prod: https://hasura-prod-xxxxx.run.app/v1/graphql
HASURA_GRAPHQL_ENDPOINT=http://localhost:8080/v1/graphql

# Hasura Admin Secret
HASURA_GRAPHQL_ADMIN_SECRET=hasura_local_admin_secret
